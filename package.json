  },
  "devDependencies": {
    "@skpm/builder": "^0.4.0",
    "@skpm/extract-loader": "^1.0.1",
    "css-loader": "^0.28.7",
    "html-loader": "^0.5.1"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "react": "^16.6.1",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.1",
    "sketch-module-web-view": "^1.2.3",
    "styled-components": "^4.0.3"
  },
  "author": "Juliano.Dasilva <me@gmail.com>"
}
