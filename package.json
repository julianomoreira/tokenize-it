{
  "name": "Tokenize-It",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Tokenize It",
    "manifest": "src/manifest.json",
    "main": "tokenize-it.sketchplugin"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
